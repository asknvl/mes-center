<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:views="using:mes_center.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="mes_center.arm_acceptorder.Views.acceptorderInterfaceView">
	
	<UserControl.Styles>
		<StyleInclude Source="/Views/Styles/WindowStyle.axaml" />
	</UserControl.Styles>

	<Grid>
		<StackPanel
			Orientation="Horizontal">			
			<views:ordersListView DataContext="{Binding orders}"/>
			<views:orderDetailsView DataContext="{Binding orderDetails}"/>
			<views:strategiesListView DataContext="{Binding strategies}"/>
		</StackPanel>

	</Grid>
	
</UserControl>
